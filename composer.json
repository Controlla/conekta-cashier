{
    "name": "controlla/conekta-cashier",
    "description": "Controlla Cashier nos da una interface para cobrar subscripciones con Conketa en Laravel.",
    "keywords": ["controlla", "conekta", "laravel", "billing"],
    "license": "MIT",
    "authors": [
        {
            "name": "controlla",
            "email": "developers@controlla.com",
            "homepage": "https://github.com/controlla/conekta-cashier"
        },
        {
            "name": "Rafael Masri",
            "email": "rafael.masri@controlla.com",
            "role": "Developer"
        },
        {
            "name": "Joseph Cohen",
            "email": "joseph.cohen@controlla.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "conekta/conekta-php": "4.0.*",
        "illuminate/filesystem": "5.7.*|5.8|6.0|7.0",
        "illuminate/support": "5.7.*|5.8|6.0|7.0"
    },
    "require-dev": {
        "illuminate/routing": "~5.7|5.8|6.0|7.0",
        "illuminate/view": "~5.7|5.8|6.0|7.0",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-0": {
            "Controlla\\ConektaCashier\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Controlla\\ConektaCashier\\CashierServiceProvider"
            ]
        }
    }
}
