{
    "name": "controlla/conekta-cashier",
    "description": "Controlla Cashier nos da una interface para cobrar subscripciones con Conketa en Laravel.",
    "keywords": ["controlla", "conekta", "laravel", "billing"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/controlla/conekta-cashier/issues",
        "source": "https://github.com/controlla/conekta-cashier"
    },
    "authors": [
        {
            "name": "Ivan Sotelo",
            "email": "isotelo@controlla.com.mx",
            "homepage": "http://ivansotelo.me"
        },
        {
            "name": "Controlla - Conecting Dots",
            "homepage": "http://controlla.com.mx"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "conekta/conekta-php": "dev-master",
        "illuminate/filesystem": "^6.0|^7.0|^8.0",
        "illuminate/support": "^6.0|^7.0|^8.0",
        "phpunit/phpunit": "9.5.x-dev"
    },
    "require-dev": {
        "illuminate/routing": "^6.0|^7.0|^8.0",
        "illuminate/view": "^6.0|^7.0|^8.0",
        "illuminate/contracts": "^6.0|^7.0|^8.0",
        "mockery/mockery": "^1.2",
        "orchestra/testbench": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Controlla\\ConektaCashier\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Controlla\\ConektaCashier\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Controlla\\ConektaCashier\\CashierServiceProvider"
            ]
        }
    }
}
