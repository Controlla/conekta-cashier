{
    "name": "dinkbit/conekta-cashier",
    "description": "Dinkbit Cashier nos da una interface para cobrar subscripciones con Conketa en Laravel.",
    "keywords": ["dinkbit", "conekta", "laravel", "billing"],
    "license": "MIT",
    "authors": [
        {
            "name": "dinkbit",
            "email": "developers@dinkbit.com",
            "homepage": "https://github.com/dinkbit/conekta-cashier"
        },
        {
            "name": "Rafael Masri",
            "email": "rafael.masri@dinkbit.com",
            "role": "Developer"
        },
        {
            "name": "Joseph Cohen",
            "email": "joseph.cohen@dinkbit.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "conekta/conekta-php": "dev-master",
        "illuminate/filesystem": "5.7.*",
        "illuminate/support": "5.7.*"
    },
    "require-dev": {
        "illuminate/routing": "~5.7",
        "illuminate/view": "~5.7",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-0": {
            "Dinkbit\\ConektaCashier\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    },
    "minimum-stability": "dev",
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dinkbit\\ConektaCashier\\LaravelConektaServiceProvider"
            ]
        }
    },
    "repositories": [
        {
            "url": "https://github.com/conekta/conekta-php.git",
            "type": "git"
        }
    ]
}
